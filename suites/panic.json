[
    {
        "description": "Can promote a soft error to a hard error with panic",
        "rule": { "panic": { "error": "Some error" } },
        "error": true,
        "data": null
    },
    {
        "description": "Panic with an error emitted from an operator",
        "rule": { "panic": [{ "/": [1, 0] }] },
        "error": true,
        "data": null
    },
    {
        "description": "Panic with an error pulled from context",
        "rule": { "panic": [{ "val": "x" }] },
        "error": true,
        "data": { "x": { "error": "Some error" } }
    },
    {
        "description": "Panic within an iterator",
        "rule": { "map": [[1, 2, 3], { "panic": [{ "/": [0,0] }] }] },
        "error": true,
        "data": null
    },
    {
        "description": "Panic if the user is not an admin",
        "rule": { "panic": [{ "if": [{"val": ["user", "admin"]}, true, { "error": "Not an admin" }] }] },
        "data": { "user": { "admin": false } },
        "error": true
    }
]