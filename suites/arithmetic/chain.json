    {
        "description": "Multiply can be chained with other operators (3)",
        "rule": { "*": { "map": [{ "val": "arr" }, { "val": "x" }] } },
        "data": {
            "arr": [
                { "x": 30 },
                { "x": 25 },
                { "x": 35 },
                { "x": 28 }
            ]
        },
        "result": 735000
    },
