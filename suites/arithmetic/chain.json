    "Divide",
    {
        "description": "Divide can be chained with other operators",
        "rule": { "/": { "val": "arr" } },
        "data": { "arr": [10, 5] },
        "result": 2
    },
    {
        "description": "Divide can be chained with other operators (2)",
        "rule": { "/": { "merge": [[20], 5] } },
        "data": null,
