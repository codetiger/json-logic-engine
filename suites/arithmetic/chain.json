    {
        "description": "Minus can be chained with other operators (3)",
        "rule": { "-": { "map": [{ "val": "people" }, { "val": "age" }] } },
        "data": {
            "people": [
                { "name": "John", "age": 30 },
                { "name": "Jane", "age": 25 },
                { "name": "Bob", "age": 35 },
                { "name": "Alice", "age": 28 }
            ]
        },
        "result": -58
